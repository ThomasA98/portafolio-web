---
import Header from "../components/astro-components/Header.astro";
import { GitRepositoryList } from "../components/solid-components/GitRepositoryList";
import Layout from "../layout/Layout.astro";

const githubUser = 'Tamiel';
const nameOwner = 'El Terrible Nombre Fake';
---

<Layout title={ nameOwner }>
	<Header name={ nameOwner } />
	<GitRepositoryList gitUser={ githubUser } client:load />
</Layout>

<style>
	* {
		margin: 0;
		padding: 0;
	}

	body {
		background-color: #131313;
	}
</style>